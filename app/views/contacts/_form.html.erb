<%= form_with model: @contact do |f| %>
    <div class="field">
        <%= f.label :title %>
        <%= f.text_field :title%>
    </div>
    
    <div class="field">
        <%= f.label :body%>
        <%= f.text_field :body %>
    </div>
    <div class="field">
        <%= f.label :prio%>
        <%= f.check_box :prio %>
    </div>
    <div class="actions">
        <%= f.submit %>
    </div>
<% end %>