<%= form_with model: @competences do |f| %>
    <div class="field">
        <%= f.label :title %>
        <%= f.text_field :title%>
    </div>
    <div class="field">
        <%= f.label :point%>
        <%= f.number_field :point %>
    </div>
    <div class="field">
        <%= f.label :prio%>
        <%= f.check_box :prio %>
    </div>
    <div class="actions">
        <%= f.submit %>
    </div>
<% end %>